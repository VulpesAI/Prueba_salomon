# Variables públicas para Supabase utilizadas por la app durante el desarrollo.
# Reemplaza estos valores con los de tu proyecto (process.env.NEXT_PUBLIC_* en despliegues).
NEXT_PUBLIC_DEMO_MODE=false
NEXT_PUBLIC_DEFAULT_CURRENCY=CLP
NEXT_PUBLIC_DEFAULT_LOCALE=es-CL
NEXT_PUBLIC_SUPABASE_URL=https://<tu-proyecto>.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=<tu-anon-key-publico>

# URLs públicas adicionales utilizadas por los servicios del frontend.
NEXT_PUBLIC_API_URL=http://localhost:3000
NEXT_PUBLIC_CONVERSATION_ENGINE_URL=http://localhost:8002
NEXT_PUBLIC_VOICE_GATEWAY_URL=http://localhost:8100
NEXT_PUBLIC_FINANCIAL_PROVIDER=belvo
